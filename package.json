    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.47.0",
    "eslint-config-canonical": "^41.1.7",
    "eslint-plugin-canonical": "^4.9.0",
    "git-cz": "^4.9.0",
    "husky": "8.0.3",
    "lint-staged": "13.3.0",
    "release-it": "^16.1.5",
    "typescript": "5.1.6",
    "vite": "^4.4.9",
    "vite-plugin-checker": "^0.6.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-http2-proxy": "^0.4.1",
    "vite-plugin-node-polyfills": "^0.10.0",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "release-it": {
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "infile": "CHANGELOG.md",
        "preset": {
          "name": "conventionalcommits",
          "types": [
            {
              "type": "feat",
              "section": "Features"
            },
            {
              "type": "fix",
              "section": "Bug Fixes"
            },
            {
              "type": "refactor",
              "section": "Refactor"
            },
            {
              "type": "perf",
              "section": "Performance"
            },
            {
              "type": "style",
              "section": "Visuals"
            },
            {
              "type": "docs",
              "section": "Docs"
            },
            {
              "type": "chore",
              "section": "Chore"
            },
            {
              "type": "test",
              "section": "Tests"
            }
          ]
        }
      }
    }
  },
  "packageManager": "yarn@3.6.1"
}
